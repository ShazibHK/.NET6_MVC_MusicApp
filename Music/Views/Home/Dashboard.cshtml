@model dynamic
<!DOCTYPE html>
<html>
    <header>
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
        <link rel="stylesheet" href="css/bootstrap-5.0.2-dist/css/bootstrap.min.css">
        <link rel="stylesheet" href="https://unpkg.com/flickity@2/dist/flickity.min.css">
        <link rel="stylesheet" href="css/Navbar.css">
        <link rel="stylesheet" href="css/owl.css">
        <link rel="stylesheet" href="https://cdn.datatables.net/1.10.22/css/jquery.dataTables.min.css" />

	    <!-- jQuery library file -->
	    <script type="text/javascript"
	    src="https://code.jquery.com/jquery-3.5.1.js">
	    </script>

	    <!-- Datatable plugin JS library file -->
	    <script type="text/javascript" src="https://cdn.datatables.net/1.10.22/js/jquery.dataTables.min.js">
	    </script>

	    <script>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
        <!--Library-->
        
        <script>
            // Basic example
            // Basic example
             $(document).ready(function () {
             $('#dtBasicExample').DataTable({
              "pagingType": "simple" // "simple" option for 'Previous' and 'Next' buttons only
              });
             $('.dataTables_length').addClass('bs-select');
             });</script>
       
        <!--Song-->
        <script>
		/* Initialization of datatable */
		$(document).ready(function() {
			$('#SongTable').DataTable({ });
		});
        </script>
	</script>
    </header>
    <body>

        <!--NavBar-->
         <header>
            <partial name="_Nav"/>
         </header>
   
        <partial name="_Notification" />
        <!--Statistics-->
        <div class="mx-auto" style="width:93%">
          <div class="row mt-4 ">
           <partial name="_StatisticSongPartial"/>
          </div>
        </div>
             
        <!--Buttons to Add Song and Library-->
        
        <div class="mx-auto" style="width:93%">
            <div style="background-color: #f0f8ff;" class="row mt-5 border border-info">
                <div class="col m-1 p-3 rounded">
                    <center>
                        <a asp-controller="Song" asp-action="AddSong"><button style="width: 400px;" class="botton-border btn btn-outline-secondary" type="button">Add Song</button></a>
                    </center>
                </div>
                <div class="col m-1 p-3 rounded ">
                    <center>
                        <a  asp-controller="PlayList" asp-action="AddPlayList"><button style="width: 400px;" class="botton-border btn btn-outline-secondary" type="button">Add Library</button></a>
                    </center>
                </div>
              
            </div>
        </div>
        
        <!--Manage Song-->
        <div class="mx-auto" style="width:93%;margin-top:80px">
            <div style="background-color: #f0f8ff;" class="row mt-3 p-3 border border-info">
                <div class="col-md-8 rounded">
                  
                        <h5>
                            Manange Song
                        </h5>   
              </div>
            </div>
        </div>

        <!--Manage SongTable-->
        <div class="mx-auto" style="width:95%;margin-top:30px">
            <table id="SongTable" class="table table-striped table-bordered border table-sm mt-3" cellspacing="3" width="100%">
                <thead style="margin:50px;">
                  <tr >
                    <th class="th-sm">Song Name
              
                    </th>
                    <th class="th-sm">Type
              
                    </th>
                    <th class="th-sm">Language
              
                    </th>
                    <th class="th-sm">Duration
                    </th>
                    <th class="th-sm">Year</th>
                    <th>Edit</th>
                    <th class="th-sm">
                    Delete
                    </th>
                  </tr>
                </thead>
                <tbody style="background-color: #f0f8ff;">
                @foreach (var obj in Model.Song){
                  <tr>
                <td width="20%">

                    @obj.SongName
				</td>
                 <td width="15%">
                    @obj.type
				</td>
                <td width="15%">
                    @obj.language
				</td>
                <td width="15%">
                    @obj.duration
				</td>
               <td width="15%">
                    @obj.Year
				</td>
                <td width="10%">
                    <div class="w-75 btn-group" role="group">
                        <a asp-controller="Song" asp-action="EditSong" asp-route-id="@obj.SongId"
                        class="btn btn-primary mx-2"> <i class="bi bi-pencil-square"></i> Edit</a>
					</div>
				</td>
                <td width="10%">
                    <div class="w-75 btn-group" role="group">
                        <a asp-controller="Song" asp-action="DeleteSong" asp-route-id="@obj.SongId"
                        
                        class="btn btn-primary mx-2"> <i class="bi bi-pencil-square"></i> Delete</a>
                    </div>
                </td>
			</tr>
                }
                </tbody>
              
            </table>
        </div>
       
        <!--Manage Library-->
         <div class="mx-auto " style="width:93%;margin-top:80px">
            <div style="background-color: #f0f8ff;" class="row mt-3 p-3 border border-info">
                <div class="col-md-8 rounded ">
                  
                        <h5>
                            Manange Libraray
                        </h5>   
              </div>
              
            </div>
        </div>

        <!--Manage Library Table-->
        <div class="mx-auto" style="width:95%;margin-top:30px">
            <table id="dtBasicExample" class="table table-striped table-bordered border table-sm mt-3 " cellspacing="0" width="100%">
                <thead>
                  <tr>
                    <th class="th-sm" >PlayList Name
              
                    </th>
                    <th class="th-sm">Type
              
                    </th>
                    <th class="th-sm">Edit
              
                    </th>
                    <th class="th-sm">Delete
                    </th>
                  </tr>
                </thead>
                <tbody style="background-color: #f0f8ff;">
                   @foreach(var obj in Model.PlayList)
        {
            <tr>
                <td width="40%">

                    @obj.PlayListName
				</td>
                <td width="40%">
                    @obj.type
				</td>
                <td width="10%">
                    <div class="w-75 btn-group" role="group">
                        <a asp-controller="PlayList" asp-action="EditPlayList" asp-route-id="@obj.PlayListId"
                        class="btn btn-primary mx-2"> <i class="bi bi-pencil-square"></i> Edit</a>
					</div>
				</td>
                <td width="10%">
                    <div class="w-75 btn-group" role="group">
                        <a asp-controller="PlayList" asp-action="DeletePlayList" asp-route-id="@obj.PlayListId"
                        
                        class="btn btn-primary mx-2"> <i class="bi bi-pencil-square"></i> Delete</a>
                    </div>
                </td>
			</tr>
        }
                </tbody>
              
          </table>
        </div>
    	
    </body>
</html>