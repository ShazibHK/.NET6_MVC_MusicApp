@model VerifyAuthenticatorViewModel
<div class="container m-5">
<p style="font-weight:bold;color:blue;font-size:20px">Verfiy Authenticator Code</p>

<form asp-controller="Account" asp-action="VerifyAuthenticatorCode" asp-route-returnurl="@ViewData["ReturnUrl"]" method="post" role="form">
    <div asp-validation-summary="All" class="text-danger"></div>
    <input asp-for="RememberMe" type="hidden" />
    <hr />
    <div class="form-group">
        <label asp-for="Code" class="col-md-2"></label>
        <div class="col-md-10">
            <input asp-for="Code" class="form-control" />
            <span asp-validation-for="Code" class="text-danger"></span>
        </div>
    </div>
    <div class="form-group">
        <div class=" mt-4">
            <button type="submit" class="btn btn-success">Submit</button>
        </div>
    </div>
</form>
</div>
@section Scripts{
    @{ await Html.RenderPartialAsync("_ValidationScriptsPartial"); }
}